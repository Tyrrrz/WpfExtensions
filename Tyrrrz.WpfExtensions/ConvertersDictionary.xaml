                    xmlns:converters="clr-namespace:Tyrrrz.WpfExtensions.Converters">

    <converters:ArrayToStringConverter x:Key="ArrayToStringConverter" />
    <converters:BoolAndMultiConverter x:Key="BoolAndMultiConverter" />
